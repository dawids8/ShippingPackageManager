<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="392.0" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.NewUserController">
    <children>
        <TextField fx:id="setUserName" layoutX="117.0" layoutY="49.0" promptText="Set user name" />
        <TextField fx:id="setPassword" layoutX="117.0" layoutY="88.0" promptText="Set password" />
        <Button fx:id="createNewUser" layoutX="117.0" layoutY="124.0" mnemonicParsing="false" onAction="#createNewUser" prefHeight="25.0" prefWidth="149.0" text="Create new user" />
        <Button fx:id="cancelCreatingNewUser" layoutX="308.0" layoutY="149.0" mnemonicParsing="false" onAction="#backToLoginWindow" text="Cancel" />
        <Label fx:id="isUserExist" alignment="CENTER" layoutX="-2.0" layoutY="14.0" prefHeight="27.0" prefWidth="392.0" textAlignment="CENTER" textFill="RED" wrapText="true" />
    </children>
</AnchorPane>
