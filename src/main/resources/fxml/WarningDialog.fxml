<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="111.0" prefWidth="350.0"
            stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8.0.171"
            fx:controller="controllers.WarningDialogController">
    <children>
        <Button fx:id="closeButton" cancelButton="true" layoutX="136.0" layoutY="78.0" mnemonicParsing="false"
                onAction="#handleCloseButtonAction" prefHeight="25.0" prefWidth="78.0" text="Ok"/>
        <Label fx:id="informationLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="25.0" layoutY="31.0"
               prefHeight="25.0" prefWidth="300.0" textAlignment="CENTER" textFill="#f50606">
            <font>
                <Font size="17.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
