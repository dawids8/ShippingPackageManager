<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="loginAnchorPane" prefHeight="256.0" prefWidth="350.0"
            stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8.0.171"
            fx:controller="controllers.LoginController">
    <children>
        <Button fx:id="login" layoutX="90.0" layoutY="145.0" mnemonicParsing="false" onAction="#login" prefHeight="29.0"
                prefWidth="150.0" text="Login"/>
        <PasswordField fx:id="password" layoutX="84.0" layoutY="99.0" promptText="Password">
            <font>
                <Font size="13.0"/>
            </font>
        </PasswordField>
        <TextField fx:id="user" layoutX="84.0" layoutY="63.0" promptText="User">
            <font>
                <Font size="13.0"/>
            </font>
        </TextField>
        <Label fx:id="loginLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="3.0" layoutY="27.0"
               prefHeight="20.0" prefWidth="335.0" textAlignment="CENTER" textFill="#fc0808">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <Button fx:id="signUp" layoutX="221.0" layoutY="213.0" mnemonicParsing="false" onAction="#goToSignUpWindow"
                prefHeight="29.0" prefWidth="104.0" text="Sign up"/>
    </children>
</AnchorPane>
